<h1>Listing scheduleruns</h1>

<table>
  <tr>
    <th>Schedule</th>
    <th>Start time</th>
    <th>End time</th>
    <th>Status flag</th>
    <th>Log file</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @scheduleruns.each do |schedulerun| %>
  <tr>
    <td><%= schedulerun.schedule_id %></td>
    <td><%= schedulerun.start_time %></td>
    <td><%= schedulerun.end_time %></td>
    <td><%= schedulerun.status_flag %></td>
    <td><%= schedulerun.log_file %></td>
    <td><%= link_to 'Show', schedulerun %></td>
    <td><%= link_to 'Edit', edit_schedulerun_path(schedulerun) %></td>
    <td><%= link_to 'Destroy', schedulerun, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Schedulerun', new_schedulerun_path %>
